{
  "name": "front-end-boilerplate",
  "version": "1.0.0",
  "description": "Simple workflow starter for building vanilla JS apps",
  "main": "index.js",
  "scripts": {
    "clean:dev": "rimraf development .cache",
    "dev": "parcel src/index.pug --out-dir development",
    "start": "yarn clean:dev && yarn dev",
    "clean:prod": "rimraf dist .cache",
    "build-prod": "parcel build src/index.pug",
    "build": "yarn clean:prod && parcel build src/index.pug --out-dir dist"
  },
  "repository": "https://github.com/coldboy002/front-end-boilerplate.git",
  "keywords": [
    "parcel",
    "starter-kit",
    "front-end",
    "front-end-boilerplate",
    "boilerplate"
  ],
  "author": "Vu Trong Thuy <coldboy002@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coldboy002/front-end-boilerplate/issues"
  },
  "homepage": "https://github.com/coldboy002/front-end-boilerplate#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@csstools/normalize.css": "^11.0.1",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^3.2.2",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.27.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.2.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
